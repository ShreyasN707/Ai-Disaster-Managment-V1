# Frontend Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:4000/api

# Application Configuration
VITE_APP_NAME=AI Disaster Management System
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_MODE=false

# Map Configuration (if using maps)
VITE_MAP_API_KEY=your_map_api_key_here
VITE_DEFAULT_MAP_CENTER_LAT=37.7749
VITE_DEFAULT_MAP_CENTER_LNG=-122.4194
VITE_DEFAULT_MAP_ZOOM=10

# WebSocket Configuration
VITE_WEBSOCKET_URL=ws://localhost:4000

# Feature Flags
VITE_ENABLE_ML_FEATURES=true
VITE_ENABLE_REAL_TIME_ALERTS=true
VITE_ENABLE_ANALYTICS=false

# Production Configuration
VITE_PRODUCTION_API_URL=https://your-production-api.com/api
VITE_PRODUCTION_WS_URL=wss://your-production-api.com
